package cyPath;

public class Coordinate extends OccupiedCoordinates {
		  private int positionX ;
		  private int positionY;
		 
		  
		  
		  //Constructor
		  public Coordinate(int posX,int posY) {
			 
			  this.positionX=posX;
			  this.positionX=posY;
		  }
		  
		  //getters and setters 
		  public int getPositionX() {
			  return positionX;
		  }
		  
		  public void setPositionX(int posX) {
			  this.positionX=posX;
		  }
		  
		  public int getPositionY() {
			  return positionY;
		  }
		  
		  public void setPositionY(int posY) {
			  this.positionY=posY;
		  }
		  

}
